{
  "compilerOptions": {
    "baseUrl": ".",
    "incremental": true,
    "paths": {
      "~/*": ["./src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
  },
  "exclude": ["node_modules", ".next"],
  "extends": "@untrace/tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "next.config.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/instrument.ts1"
  ]
}
