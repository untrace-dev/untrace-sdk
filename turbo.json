{
  "$schema": "https://turborepo.org/schema.json",
  "concurrency": "20",
  "globalEnv": ["NODE_ENV"],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event",
    "TURBO_TOKEN",
    "TURBO_TEAM",
    "NPM_TOKEN",
    "VERCEL_TOKEN"
  ],
  "tasks": {
    "@untrace/sdk-csharp#build": {
      "dependsOn": ["^build"],
      "outputs": ["bin/**", "obj/**"]
    },
    "@untrace/sdk-csharp#pack": {
      "dependsOn": ["^build"],
      "outputs": ["*.nupkg"]
    },
    "@untrace/sdk-csharp#test": {
      "dependsOn": ["^build"],
      "outputs": ["TestResults/**", "coverage/**"]
    },
    "@untrace/sdk-elixir#build": {
      "dependsOn": ["^build"],
      "outputs": ["_build/**"]
    },
    "@untrace/sdk-elixir#format": {
      "dependsOn": ["^build"]
    },
    "@untrace/sdk-elixir#lint": {
      "dependsOn": ["^build"]
    },
    "@untrace/sdk-elixir#test": {
      "dependsOn": ["^build"],
      "outputs": ["cover/**"]
    },
    "@untrace/sdk-elixir#typecheck": {
      "dependsOn": ["^build"],
      "outputs": ["_build/dialyzer/**"]
    },
    "@untrace/sdk-go#build": {
      "dependsOn": ["^build"],
      "outputs": ["*.exe", "*.so", "*.dylib"]
    },
    "@untrace/sdk-go#test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage.out", "coverage.html"]
    },
    "@untrace/sdk-python#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "@untrace/sdk-python#typecheck": {
      "outputs": [".mypy_cache/**"]
    },
    "@untrace/sdk-rust#build": {
      "dependsOn": ["^build"],
      "outputs": ["target/**"]
    },
    "@untrace/sdk-rust#check": {
      "dependsOn": ["^build"],
      "outputs": ["target/**"]
    },
    "@untrace/sdk-rust#test": {
      "dependsOn": ["^build"],
      "outputs": ["target/**"]
    },
    "//#clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_*",
        "CLERK_*",
        "SUPABASE_*",
        "POSTGRES_*",
        "POSTHOG_*",
        "OPENAI_API_KEY"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**",
        ".output/**",
        ".vercel/output/**",
        "dist/**",
        "build/**",
        "bin/**",
        "*.generated.ts"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "env": [
        "PORT",
        "NEXT_PUBLIC_*",
        "CLERK_*",
        "SUPABASE_*",
        "POSTGRES_*",
        "POSTHOG_*",
        "OPENAI_API_KEY"
      ],
      "persistent": true
    },
    "format": {
      "dependsOn": ["^build"]
    },
    "format:ci": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"],
      "env": ["USE_LOCAL_DB"],
      "outputs": ["coverage/**"]
    },
    "test:ci": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:watch": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "typecheck": {
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    }
  },
  "ui": "tui"
}
